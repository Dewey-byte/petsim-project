<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="folder2.PrimaryController">
   <children>
      <ImageView fx:id="bg_img" preserveRatio="true" StackPane.alignment="CENTER">
         <image>
            <Image url="@background.png" />
         </image>
      </ImageView>
      <ImageView fx:id="welcome_img" fitHeight="251.0" fitWidth="421.0" preserveRatio="true" StackPane.alignment="TOP_CENTER"> 
         <image> 
             <Image url="@welcome.png" />
         </image>
         <StackPane.margin>
            <Insets top="50.0" />
         </StackPane.margin>
         <effect>
            <Blend mode="MULTIPLY" />
         </effect>
      </ImageView>
     <ImageView fx:id="start_img" fitHeight="98.0" fitWidth="229.0" onMouseClicked="#switchToSecondary" preserveRatio="true" StackPane.alignment="BOTTOM_CENTER"> 
         <image> 
             <Image url="@Press_Start.png" />
         </image>
         <effect>
            <Blend />
         </effect>
         <StackPane.margin>
            <Insets bottom="150.0" />
         </StackPane.margin>
     </ImageView>
   </children>
</StackPane>
