<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="folder2.TertiaryController">
   <children>
      <ImageView fx:id="bg_img" preserveRatio="true" StackPane.alignment="CENTER">
         <image>
            <Image url="@background.png" />
         </image>
      </ImageView>
      <Button mnemonicParsing="false" onMouseClicked="#switchToSecondary" prefHeight="49.0" prefWidth="147.0" text="switchToSecondary" StackPane.alignment="BOTTOM_CENTER">
         <StackPane.margin>
            <Insets bottom="70.0" />
         </StackPane.margin>
      </Button>
      <ImageView fx:id="cat_img" fitHeight="177.0" fitWidth="173.0" preserveRatio="true" StackPane.alignment="CENTER_RIGHT">
         <image>
            <Image url="@cat.png" />
         </image>
         <StackPane.margin>
            <Insets right="120.0" top="10.0" />
         </StackPane.margin>
      </ImageView>
      <ImageView fx:id="dog_img" fitHeight="180.0" fitWidth="168.0"  preserveRatio="true" StackPane.alignment="CENTER">
         <image>
            <Image url="@dog.png" />
         </image>
         <StackPane.margin>
            <Insets top="10.0" />
         </StackPane.margin>
      </ImageView>
      <ImageView fx:id="bunny_img" fitHeight="180.0" fitWidth="168.0"  preserveRatio="true" StackPane.alignment="CENTER_LEFT">
         <image>
            <Image url="@bunny.png" />
         </image>
         <StackPane.margin>
            <Insets left="120.0" top="10.0" />
         </StackPane.margin>
      </ImageView>
       <ImageView fx:id="Choosepet_img" fitHeight="67.0" fitWidth="532.0" preserveRatio="true" StackPane.alignment="TOP_CENTER">
        <image>
            <Image url="@ChoosePet.png" />
        </image>
      <StackPane.margin>
         <Insets top="120.0" />
      </StackPane.margin>
      </ImageView>
   </children>
</StackPane>
